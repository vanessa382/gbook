<p id="notice"><%= notice %></p>

<h1>Listing Gbooks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gbooks.each do |gbook| %>
      <tr>
        <td><%= link_to 'Show', gbook %></td>
        <td><%= link_to 'Edit', edit_gbook_path(gbook) %></td>
        <td><%= link_to 'Destroy', gbook, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gbook', new_gbook_path %>
